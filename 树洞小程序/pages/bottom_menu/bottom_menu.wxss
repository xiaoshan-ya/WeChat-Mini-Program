.bottomRow {
    background-color: white;
    width: 100%;
    height: 9%;
    bottom: 0rpx;
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
    position: fixed; /*加上位置才能使这一行在最低端*/
}

.bottomTab {
    width: 250rpx;
    display: flex;
     flex-direction: column;
     justify-content: center;
     align-items: center;

}

.bottomImage {
    margin-top: 10rpx;
    width: 130rpx;
    height: 110rpx;
    border-radius: 10rpx;
}

.bottomTabText {
    font-size: 35rpx;
    color: #979797;

}